	Pmid = {25959748},
	Pst = {ppublish},
	Title = {Privileged scaffolds in lead generation},
	Volume = {10},
	Year = {2015},
	Bdsk-Url-1 = {http://dx.doi.org/10.1517/17460441.2015.1041496}}

