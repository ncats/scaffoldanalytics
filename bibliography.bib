	Issn = {0028-0836},
	Journal = {Nature},
	Number = {7296},
	Pages = {305--310},
	Pmid = {20485427},
	Title = {{Thousands of chemical starting points for antimalarial lead identification.}},
	Url = {http://dx.doi.org/10.1038/nature09107},
	Volume = {465},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nature09107}}

